  },
  "main": "dist/dv-social-share.js",
  "module": "dist/dv-social-share.es.js",
  "jsnext:main": "dist/dv-social-share.es.js",
  "types": "dist/src/index.d.ts",
  "@comment unpkg": "dist/dv-social-share.umd.js",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "deleteDist": "rimraf dist",

{
  "name": "dv-social-share",
  "version": "0.0.6",
  "description": "Social media share buttons for your next apps like React, Next.js, Vue.js, Angular written in TypeScript.",
  "author": "Dhaval Vira <dhavalveera@gmail.com> (https://github.com/dhavalveera)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhavalveera/dv-social-share.git"
  },
  "bugs": {
    "url": "https://github.com/dhavalveera/dv-social-share/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "react.js",
    "next",
    "nextjs",
    "next.js",
    "vue",
    "vuejs",
    "vue.js",
    "social-share-links",
    "share-button",
    "social-share",
    "social-media-share-link",
    "share-blogs",
    "share-posts",
    "share-blogs-on-social"
